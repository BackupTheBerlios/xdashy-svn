# sample menu.config file
# make a mistake and xdashy won't start at all
# make a serious mistake and expect the worse

"Apps"
{
	# search directory "e:\apps\" and add a new menu
	# entry for each directory that contains a 
	# "default.xbe"
	"AUTO ADD" = "e:\apps\"
}

"Games"
{
	"AUTO ADD" = "f:\games\"
}

"Emulators"
{
	"AUTO ADD" = "e:\Emulators"
}

# other dashboards
"MS dashboard" = "c:\msxboxdash.xbe"
"EVOX dashboard" = "c:\evox.xbe"

# "REBOOT XBOX" is a special command
"Reboot" = "REBOOT XBOX"
